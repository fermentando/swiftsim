  $as_echo_n "(cached) " >&6
else
  case $GIT_CMD in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_GIT_CMD="$GIT_CMD" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_GIT_CMD="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
GIT_CMD=$ac_cv_path_GIT_CMD
if test -n "$GIT_CMD"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $GIT_CMD" >&5
$as_echo "$GIT_CMD" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi




