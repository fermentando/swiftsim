#  Cooling function

# Check whether --with-cooling was given.
if test "${with_cooling+set}" = set; then :
  withval=$with_cooling; with_cooling="$withval"
else
  with_cooling="none"

fi


if test "$with_subgrid" != "none"; then
   if test "$with_cooling" != "none"; then
      as_fn_error $? "Cannot provide with-subgrid and with-cooling together" "$LINENO" 5
   else
      with_cooling="$with_subgrid_cooling"
   fi
fi

with_cooling_name="none"
case "$with_cooling" in
   none)

$as_echo "#define COOLING_NONE 1" >>confdefs.h

   ;;
   const-du)

$as_echo "#define COOLING_CONST_DU 1" >>confdefs.h

      with_cooling_name=$with_cooling
   ;;
   const-lambda)

$as_echo "#define COOLING_CONST_LAMBDA 1" >>confdefs.h

      with_cooling_name=$with_cooling
   ;;
   grackle_*)

      if test "$have_grackle" != "yes"; then
        as_fn_error $? "Grackle cooling: You need the grackle library for Grackle cooling. (--with-grackle=PATH)" "$LINENO" 5
      fi


$as_echo "#define COOLING_GRACKLE 1" >>confdefs.h

      primordial_chemistry=${with_cooling#*_}

cat >>confdefs.h <<_ACEOF
#define COOLING_GRACKLE_MODE $primordial_chemistry
_ACEOF

      with_cooling_name="Grackle $primordial_chemistry"
      with_cooling="grackle"
   ;;
   SIMBA)

$as_echo "#define COOLING_SIMBA 1" >>confdefs.h


$as_echo "#define COOLING_GRACKLE_MODE 1" >>confdefs.h

      with_cooling_name="SIMBA (Grackle 3+ with extensions)"
   ;;
   KIARA)

$as_echo "#define COOLING_KIARA 1" >>confdefs.h


$as_echo "#define COOLING_GRACKLE_MODE 2" >>confdefs.h

      with_cooling_name="KIARA (Grackle 3.2+ incl. dust+H2 formation)"
   ;;
   QLA)

$as_echo "#define COOLING_QLA 1" >>confdefs.h

      with_cooling_name="QLA (Ploeckinger+20 tables) with constant primordial Z"
   ;;
   QLA-EAGLE)

$as_echo "#define COOLING_QLA_EAGLE 1" >>confdefs.h

      with_cooling_name="QLA (Wiersma+09 tables) with constant primordial Z"
   ;;
   EAGLE)

$as_echo "#define COOLING_EAGLE 1" >>confdefs.h

      with_cooling_name="EAGLE (Wiersma+09 tables)"
   ;;
   Wiersma)

$as_echo "#define COOLING_EAGLE 1" >>confdefs.h

      with_cooling_name="EAGLE (Wiersma+09 tables)"
   ;;
   PS2020)

$as_echo "#define COOLING_PS2020 1" >>confdefs.h

      with_cooling_name="PS2020 (Ploeckinger+20 tables)"
   ;;
   COLIBRE)
      as_fn_error $? "This cooling model changed name. Please use 'PS2020'." "$LINENO" 5
   ;;
   *)
      as_fn_error $? "Unknown cooling function: $with_cooling" "$LINENO" 5
   ;;
esac

if test "$with_cooling" = "EAGLE" || test "$with_cooling" = "PS2020"; then
   if test "$with_chemistry" = "none"; then
      as_fn_error $? "Cannot run with EAGLE or PS2020 cooling without chemistry. Please pick a chemistry model" "$LINENO" 5
   fi
fi

